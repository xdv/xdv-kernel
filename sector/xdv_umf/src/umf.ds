        return 0;
    }

    proc K::alloc_qstate_memory() {
        return 0;
    }

    proc K::alloc_phi_state_memory() {
        return 0;
    }

    proc K::alloc_shared_memory() {
        return 0;
    }

    proc K::domain_free() {
        return 0;
    }

    proc K::clone_qstate() {
        return 0;
    }

    proc K::transfer_qstate() {
        return 0;
    }

    proc K::validate_phi_coherence() {
        return 1;
